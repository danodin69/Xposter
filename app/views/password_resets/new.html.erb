<h1> Forgot Password</h1>

<%= form_with url:edit_password_reset_path do |form| %>
    <div class="mb-3">
            <%=form.label :email %>
            <%= form.text_field :email, class:"form-control", placeholder: "banana@monkey.com" %>
    </div>

    <div class="mb-3">
        <%= form.submit "Reset", data: { turbo_frame: 'signup_frame'},class:"btn btn-success"%>
    </div>
<%end%>